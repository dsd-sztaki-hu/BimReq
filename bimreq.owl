#################################################################
#    Object Properties
#################################################################

###  http://demo.dsd.sztaki.hu/bimreq#basedOn
:basedOn rdf:type owl:ObjectProperty ;
         rdfs:domain :Report ;
         rdfs:range :RequirementSet .


###  http://demo.dsd.sztaki.hu/bimreq#contains
:contains rdf:type owl:ObjectProperty ;
          owl:inverseOf :inSet ;
          rdfs:domain :RequirementSet ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :Requirement
                                     :RequirementSet
                                   )
                     ] .


###  http://demo.dsd.sztaki.hu/bimreq#hasResult
:hasResult rdf:type owl:ObjectProperty ;
           rdfs:domain :Report ;
           rdfs:range sh:ValidationReport .


###  http://demo.dsd.sztaki.hu/bimreq#inSet
:inSet rdf:type owl:ObjectProperty ;
       rdfs:domain [ rdf:type owl:Class ;
                     owl:unionOf ( :Requirement
                                   :RequirementSet
                                 )
                   ] ;
       rdfs:range :RequirementSet .


###  http://demo.dsd.sztaki.hu/bimreq#issuer
:issuer rdf:type owl:ObjectProperty ;
        rdfs:range foaf:Agent .


###  http://demo.dsd.sztaki.hu/bimreq#sourceRegulation
:sourceRegulation rdf:type owl:ObjectProperty ;
                  rdfs:domain :Requirement ;
                  rdfs:range :Regulation .


###  http://demo.dsd.sztaki.hu/bimreq#validatorShape
:validatorShape rdf:type owl:ObjectProperty ;
                rdfs:domain :Requirement ;
                rdfs:range sh:Shape .


